{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python manage.py migrate && gunicorn edu2job_backend.wsgi:application --bind 0.0.0.0:$PORT --access-logfile - --error-logfile - --timeout 120",
    "healthcheckPath": "/health/",
    "healthcheckTimeout": 30,
    "healthcheckInterval": 60
  }
}
